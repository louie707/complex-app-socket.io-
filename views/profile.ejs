 <%- include('includes/header')%>
 <!-- header ends here -->
  
 <div class="container py-md-5 container--narrow">

  <%- include('includes/flash') %>

    <%- include('includes/profileShared') %>
  
    <div class="list-group">
      <% posts.forEach((post) => { %>
        <a href="/post/<%= post._id %>" class="list-group-item list-group-item-action">
          <img class="avatar-tiny" src="<%= post.author.avatar %>">
          <strong><%= post.title %></strong> on <%= post.dateCreated.getMonth() +1 %>/<%= post.dateCreated.getDate() %>/<%= post.dateCreated.getFullYear() %>
        </a>
      <% }) %>
    </div>
  </div>
  
  <!-- footer begins -->
  <%- include('includes/footer')%>